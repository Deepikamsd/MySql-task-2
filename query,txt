create table course(course_id int primary key , course_name varchar(30));
create table student(std_id int primary key , first_name varchar(30),last_name varchar(30), 
course_id int , 
foreign key(course_id) references course(course_id)); 
create table fees(fees_id int primary key,
std_id int, 
foreign key(std_id) references student(std_id) ,
course_id int , 
foreign key(course_id) references course(course_id));

describe course;
describe student;
describe fees;